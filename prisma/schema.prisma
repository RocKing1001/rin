generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model invites {
  invite_key String    @id @unique
  creation   DateTime  @default(now()) @db.Timestamp(6)
  expire     DateTime? @db.Timestamp(6)
  desc       String?
  created_by String?
  uses       Int       @default(1)
}
